<p id="notice"><%= notice %></p>

<p>
  <strong>Session:</strong>
  <%= @session_councilman.session.date.to_time.strftime('%d/%m/%Y') %>
</p>

<p>
  <strong>Councilman:</strong>
  <%= @session_councilman.councilman.name %>
</p>

<p>
  <strong>Arrival:</strong>
  <%= @session_councilman.arrival.to_time.strftime('%H:%M') %>
</p>

<p>
  <strong>Leaving:</strong>
  <%= @session_councilman.leaving.to_time.strftime('%H:%M') %>
</p>

<p>
  <strong>Note:</strong>
  <%= @session_councilman.note %>
</p>

<%= link_to 'Edit', edit_session_councilman_path(@session_councilman) %> |
